
html, body {
    min-height:100%;
    height: 100%;
}

body {
	padding: 0px;
	margin: 0px;
	overflow-x: hidden;
}
.content {
  margin-top: 50px;
}
@media (max-width: 979px) {
  .content {
    margin-top: 50px;
  }
}
.content {
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    position: absolute;
    width: auto;
    height: auto;
}

.container-fluid {
  padding: 0;
  width: 100%;
  height: 100%;
  min-height: 100%;
}

.section-expand {
  height: 100%;
  min-height: 100%;
}

.page-header {
    padding-top: 0px;
}

.undecorated-link:hover {
	text-decoration: none;
}
[ng\:cloak], [ng-cloak], [data-ng-cloak], [x-ng-cloak], .ng-cloak, .x-ng-cloak {
	display: none !important;
}

#homeMap {
    z-index: 0;
}

.logo {
    z-index: 100;    
}

.logo img {
    padding: 50px;
    position:relative;
}

.angular-leaflet-map {
    position:absolute;
    width: 100%;   
}

.Absolute-Center {
  min-height:40%;
  max-height: 80%;
  min-width: 30%;
  max-width: 80%;
  margin: auto;
  position: absolute;
  top: 0; left: 0; bottom: 0; right: 0;
}

.image { 
   position: relative; 
   width: 100%; 
}

.landing-block { 
    position: absolute; 
    top: 10%; 
    left: 25%; 
    width: 50%; 
	height: 55%;
	padding: 15px;
    background-color:#E6E6FA;
    background: rgba(255,255,255,0.8);
    box-shadow: 0 0 15px rgba(0,0,0,0.2);
    border-radius: 5px;
	}


a.btn {
  background: #cccccc;
  -webkit-border-radius: 28;
  -moz-border-radius: 28;
  border-radius: 5px;
  font-family: Arial;
  color: #ffffff;
  font-size: 10px;
  padding: 5 15 5 15;
  text-decoration: none;
}

a.btn:hover {
  background-image: -webkit-linear-gradient(top, #3cb0fd, #3498db);
  background-image: -moz-linear-gradient(top, #3cb0fd, #3498db);
  background-image: -ms-linear-gradient(top, #3cb0fd, #3498db);
  background-image: -o-linear-gradient(top, #3cb0fd, #3498db);
  background-image: linear-gradient(to bottom, #3cb0fd, #3498db);
  text-decoration: none;
}

#ribbon1,#ribbon2 {
    position: fixed;
    right: 0;
    bottom: 60px;
}
#ribbon2 {
	bottom: 10px;
}

/* Base candidates

-------------------------------------------------- */

a:focus {
    outline: none;
}

.dark .stroke {
    border: 2px solid rgba(255, 255, 255, .25);
}

label.inline {
    margin: 0 10px;
    line-height: 40px;
}

.center-v {
    top: 50%;
    -webkit-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    transform: translateY(-50%);
}

.load-container {
    position: relative;
    min-height: 400px;
}

.pad0x {
    padding-left: 0;
    padding-right: 0;
}

.pad10x {
    padding-left: 20px;
    padding-right: 20px;
}
^X

.center {
    text-align: center;
}

.button {
	font-size: 14px;
    font-weight: 600;
    color: #fff;
    background: #3498db;
    text-align: center;
}

.button:hover, .button:focus {
    text-decoration: none;
    color: #fff;
    opacity: 0.8;
}

.button.white {
    color: #3498db;
    background: #fff;
}

.button.blue {
    color: #3c4c57;
    background: none;
}

.button.stroke {
    border: 3px solid #fff;
}

.button.stroke.blue {
    border: 3px solid #3c4c57;
    line-height: 38px;
    color: #3c4c57;
}

.scroll {
    clear: both;
    display: block;
    height: 53px;
    margin-top: -53px;
}

.search-toggle {
    display: none;
}

.ui-autocomplete {
    z-index: 1200;
}

/* Prevents animation on initial page load. See lib.app.js */

body:not(.animate) .animate {
    -webkit-transition: none;
    -moz-transition: none;
    -ms-transition: none;
    transition: none;
}

.text-aqua {
    color: #3bb2d0;
}

